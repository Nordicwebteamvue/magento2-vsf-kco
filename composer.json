{
  "name": "kodbruket/magento2-vsf-kco",
  "type": "magento2-component",
  "license": "MIT License",
  "authors": [
    {
      "name": "Huy Nguyen",
      "email": "huy@kodbruket.se"
    },
    {
      "name": "Andrii Glushych",
      "email": "andrii@kodbruket.se"
    },
    {
      "name": "Andreas Karlsson",
      "email": "andreas@kodbruket.se"
    }
